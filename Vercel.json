{
  "version": 2,
  "builds": [
    { "src": "/api/HotNews", "use": "@vercel/node" },
    { "src": "/api/ListNews", "use": "@vercel/node" },
    { "src": "/api/Olahraga", "use": "@vercel/node" },
    { "src": "/api/Pertanian", "use": "@vercel/node" },
    { "src": "/api/Pendidikan", "use": "@vercel/node" }
  ],
  "routes": [
    { "src": "/api/HotNews", "dest": "PertanianControllers.js" },
    { "src": "/api/ListNews", "dest": "ListNewsControllers.js" },
    { "src": "/api/Olahraga", "dest": "OlahragaControllers.js" },
    { "src": "/api/Pendidikan", "dest": "PendidikanControllers.js" },
    { "src": "/api/Pertanian", "dest": "PertanianControllers.js" },
    { "src": "/(.*)", "dest": "api/index.js" }
  ],
  "env": {
    "NODE_ENV": "production"
  }
}
